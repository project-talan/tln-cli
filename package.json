{
  "name": "tln-cli",
  "version": "1.37.0",
  "description": "Advanced Component Management System",
  "main": "cli.js",
  "scripts": {
    "test": "nyc --reporter=text --reporter=lcov mocha **/*spec.js",
    "update": "node update.js"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js"
    ]
  },
  "bin": {
    "tln": "cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/project-talan/tln-cli.git"
  },
  "author": "Vladyslav Kurmaz",
  "license": "gpl-3.0",
  "bugs": {
    "url": "https://github.com/project-talan/tln-cli/issues"
  },
  "homepage": "https://github.com/project-talan/tln-cli#readme",
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "compare-versions": "^3.6.0",
    "dotenv": "^8.6.0",
    "empty-dir": "^2.0.0",
    "find-up": "^4.1.0",
    "getos": "^3.2.1",
    "log4js": "^6.3.0",
    "node-fetch": "^2.6.6",
    "tmp": "^0.2.1",
    "yaml": "^1.10.2",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "json-fn": "^1.1.1",
