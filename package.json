{
  "name": "tln-cli",
  "version": "2.0.0",
  "description": "Advanced Component Management System",
  "main": "cli.js",
  "scripts": {
    "test": "nyc --reporter=text --reporter=lcov mocha src/*spec.js",
    "update": "node update.js"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js",
      "src/logger.js",
      "cli.js"
    ]
  },
  "bin": {
    "tln": "./cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/project-talan/tln-cli.git"
  },
  "author": "Vladyslav Kurmaz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/project-talan/tln-cli/issues"
  },
  "homepage": "https://github.com/project-talan/tln-cli#readme",
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "compare-versions": "^3.6.0",
    "dotenv": "^8.2.0",
    "empty-dir": "^2.0.0",
    "find-up": "^4.1.0",
    "getos": "^3.2.0",
    "line-reader": "^0.4.0",
    "log4js": "^6.4.2",
    "node-fetch": "^2.6.7",
    "require-from-string": "^2.0.2",
    "tmp": "^0.2.1",
    "yaml": "^1.8.3",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "json-fn": "^1.1.1",
    "mocha": "^9.2.1",
    "mock-fs": "^4.13.0",
    "nyc": "^15.0.0",
    "sinon": "^9.0.0"
  }
}
